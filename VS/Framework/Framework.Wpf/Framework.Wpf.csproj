<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>Framework.Wpf</RootNamespace>
    <AssemblyName>Framework.Wpf</AssemblyName>
	<TargetFramework>net461</TargetFramework>
  </PropertyGroup>
